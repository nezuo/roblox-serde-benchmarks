            }
        },

        "ReplicatedStorage": {
            "Client": {
                "$path": "src/client"
            },
            "Shared": {
                "$path": "src/shared"
            },
            "runClient": {
                "$path": "src/runClient.client.lua"
            },
            "Packages": {
                "$path": "Packages"
            },
            "RemoteEvent": {
                "$className": "RemoteEvent"
            },
            "ZAP_RELIABLE": {
                "$className": "RemoteEvent"
            },
            "ZAP_UNRELIABLE": {
                "$className": "UnreliableRemoteEvent"
            }
        }
    }
}
